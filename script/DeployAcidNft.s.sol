pragma solidity ^0.8.20;

import {Script} from "forge-std/Script.sol";
import {AcidNft} from "../src/AcidNft.sol";

contract DeployAcidNft is Script {
    string public constant NORMAL_TRIP_SVG_URI =
        "data:image/svg+xml;base64,";
    string public constant HEROIC_TRIP_SVG_URI =
        "data:image/svg+xml;base64,";

    function run() external returns (AcidNft acidNft) {
        vm.startBroadcast();
        acidNft = new AcidNft(NORMAL_TRIP_SVG_URI, HEROIC_TRIP_SVG_URI);
        vm.stopBroadcast();
    }
}
